// Natural numbers

let Nat : Type
  = fun (Nat : _) ->
      fun (succ : Nat -> Nat) ->
      fun (zero : Nat) ->
        Nat;

let zero : Nat
  = fun Nat succ zero => zero;

let succ : Nat -> Nat
  = fun prev =>
      (fun Nat succ zero =>
        succ (prev _ succ zero));

let add : Nat -> Nat -> Nat
  = fun n0 n1 Nat succ zero =>
      n0 Nat succ (n1 Nat succ zero);

let mul : Nat -> Nat -> Nat
  = fun n0 n1 =>
      fun Nat succ zero =>
        n0 Nat (n1 Nat succ) zero;

